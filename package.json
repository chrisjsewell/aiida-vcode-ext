{
    "name": "aiida-explore-vscode",
    "displayName": "AiiDA-Explorer",
    "description": "VSCode extension for exploring an AiiDA repository",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/chrisjsewell/aiida-vcode-ext.git"
    },
    "bugs": {
        "url": "https://github.com/chrisjsewell/aiida-vcode-ext/issues/new/choose"
    },
    "homepage": "https://github.com/chrisjsewell/aiida-vcode-ext/blob/master/README.md",
    "publisher": "chrisjsewell",
    "keywords": [
        "aiida",
        "repository"
    ],
    "authors": [
        "Chris Sewell"
    ],
    "license": "MIT",
    "engines": {
        "vscode": "^1.41.0",
        "node": ">=10"
    },
    "categories": [
        "Other"
    ],
    "icon": "resources/aiida-icon.png",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "aiida-explorer",
                    "title": "AiiDA Explorer",
                    "icon": "resources/aiida-icon2.svg"
                }
            ]
        },
        "views": {
            "aiida-explorer": [
                {
                    "id": "aiidaComputerCodes",
                    "name": "Codes"
                },
                {
                    "id": "aiidaGroupNodes",
                    "name": "Groups"
                },
                {
                    "id": "aiidaProcess",
                    "name": "Processes"
                },
                {
                    "id": "aiidaMisc",
                    "name": "Miscellaneous"
                }
            ]
        },
        "viewsWelcome": [
            {
                "view": "aiidaComputerCodes",
                "contents": "No Computers found, check database connection?\n[Open Settings](command:workbench.action.openSettings?%5B%22aiida%22%5D)"
            },
            {
                "view": "aiidaProcess",
                "contents": "No Processes found, check database connection?\n[Open Settings](command:workbench.action.openSettings?%5B%22aiida%22%5D)"
            },
            {
                "view": "aiidaGroupNodes",
                "contents": "No Groups found, check database connection?\n[Open Settings](command:workbench.action.openSettings?%5B%22aiida%22%5D)"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "aiidaComputerCodes.refreshEntry",
                    "when": "view == aiidaComputerCodes",
                    "group": "navigation"
                },
                {
                    "command": "aiidaGroupNodes.refreshEntry",
                    "when": "view == aiidaGroupNodes",
                    "group": "navigation"
                },
                {
                    "command": "aiidaProcess.toggleGroupBy",
                    "when": "view == aiidaProcess",
                    "group": "navigation"
                },
                {
                    "command": "aiidaProcess.refreshEntry",
                    "when": "view == aiidaProcess",
                    "group": "navigation"
                },
                {
                    "command": "aiidaMisc.refreshEntry",
                    "when": "view == aiidaMisc",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "aiida.inspectNode",
                    "when": "viewItem == node"
                },
                {
                    "command": "aiida.inspectProcess",
                    "when": "viewItem == process"
                },
                {
                    "command": "aiida.inspectProcessLogs",
                    "when": "viewItem == process"
                },
                {
                    "command": "aiida.inspectComputer",
                    "when": "viewItem == computer"
                },
                {
                    "command": "aiida.inspectGroup",
                    "when": "viewItem == group"
                }
            ]
        },
        "commands": [
            {
                "command": "aiidaComputerCodes.refreshEntry",
                "title": "AiiDA: Refresh Codes",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "command": "aiidaGroupNodes.refreshEntry",
                "title": "AiiDA: Refresh Groups",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "command": "aiidaProcess.refreshEntry",
                "title": "AiiDA: Refresh Processes",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "command": "aiidaProcess.toggleGroupBy",
                "title": "AiiDA: Toggle GroupBy",
                "icon": {
                    "light": "resources/light/tag.svg",
                    "dark": "resources/dark/tag.svg"
                }
            },
            {
                "command": "aiidaMisc.refreshEntry",
                "title": "AiiDA: Refresh Settings",
                "icon": {
                    "light": "resources/light/refresh.svg",
                    "dark": "resources/dark/refresh.svg"
                }
            },
            {
                "command": "aiida.inspectComputer",
                "title": "Inspect"
            },
            {
                "command": "aiida.inspectNode",
                "title": "Inspect"
            },
            {
                "command": "aiida.inspectProcess",
                "title": "Inspect"
            },
            {
                "command": "aiida.inspectProcessLogs",
                "title": "Logs"
            },
            {
                "command": "aiida.inspectGroup",
                "title": "Inspect"
            }
        ],
        "configuration": {
            "title": "AiiDA-Repository Configuration",
            "properties": {
                "aiida.connection": {
                    "description": "parameters for connecting to the postgres database",
                    "type": "object",
                    "required": [
                        "host",
                        "port",
                        "user",
                        "database"
                    ],
                    "properties": {
                        "host": {
                            "type": "string",
                            "default": "localhost"
                        },
                        "port": {
                            "type": "number",
                            "default": 5432
                        },
                        "user": {
                            "type": "string",
                            "default": "aiida"
                        },
                        "database": {
                            "type": "string",
                            "default": "aiida_db"
                        },
                        "password": {
                            "type": "string",
                            "default": "password"
                        },
                        "timeout_ms": {
                            "type": "number",
                            "default": 1000,
                            "description": "timeout connecting after x milliseconds"
                        }
                    },
                    "default": {
                        "host": "localhost",
                        "port": 5432,
                        "user": "aiida",
                        "database": "aiida_db",
                        "password": "password",
                        "timeout_ms": 1000
                    }
                },
                "aiida.query_max": {
                    "description": "maximum records to return from a single query",
                    "type": "number",
                    "default": 10000
                }
            }
        },
        "snippets": [
            {
                "language": "python",
                "path": "snippets/aiida_core.json"
            }
        ]
    },
    "main": "out/extension",
    "activationEvents": [
        "onView:aiidaComputerCodes"
    ],
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^8.10.25",
        "@types/nunjucks": "^3.1.3",
        "@types/vscode": "^1.41.0",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.24.0",
        "eslint": "^6.8.0",
        "eslint-plugin-import": "^2.20.1",
        "minimist": ">=1.2.2",
        "mocha": "^6.1.4",
        "nunjucks": "^3.2.1",
        "process": "^0.11.10",
        "tslint": "^5.8.0",
        "typescript": "^3.5.1",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "@types/js-yaml": "^3.12.4",
        "@types/lodash": "^4.14.155",
        "esnext": "^3.3.1",
        "js-yaml": "^3.14.0",
        "lodash": "^4.17.15",
        "ts-postgres": "^1.1.3",
        "vscode-nls": "^4.1.2"
    },
    "scripts": {
        "lint": "eslint --cache --ext .ts,.js .",
        "lint:fix": "eslint --fix --cache --ext .ts,.js .",
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node out/test/runIntergration.js && mocha --ui tdd out/test/standalone/"
    }
}
